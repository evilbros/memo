# app name
NAME=test

# pack command
PACK_CMD="./dev.sh pack"

# packed bin directory
BIN_DIR=bin

# volumes
VOLUMES=(
    back/config.json
    back/data
)

# nginx proxies
PROXIES=(
    "/:1234"
    "/back:1234"
)

# nginx statics
STATICS=(
    "/ui:/front"
    "/front/go:/front"
)

# commands
START="cd back; ./ctl.sh start"
STOP="cd back; ./ctl.sh stop"

