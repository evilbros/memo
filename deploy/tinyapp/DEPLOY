# app name
NAME=test

# packed tar file
TAR_FILE=bin.tar.gz

# volumes
VOLUMES=(
    back/config.json
    back/data
)

# nginx proxies
PROXIES=(
    "/:1234"
    "/back:1234"
)

# nginx statics
STATICS=(
    "/ui:/front"
    "/front/go:/front"
)

# commands
START="cd back; ./ctl.sh start"
STOP="cd back; ./ctl.sh stop"

